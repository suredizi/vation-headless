<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <menuItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        cq-msm-lockable="./menuIconItems"
                                        fieldDescription="Click 'Add' to Menu items."
                                        fieldLabel="Menu Icon Items"
                                        name="./menuIconItems">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            name="./menuIconItems">
                                            <items jcr:primaryType="nt:unstructured">
                                                <headingIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="Select path of the Cart icon"
                                                    fieldLabel="Heading Icon"
                                                    name="./headingIcon"
                                                    required="false"
                                                    value=""/>
                                                <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Add Heading of the menu item"
                                                    fieldLabel="Heading"
                                                    name="./heading"/>
                                                <headingNavigationUrl
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Add Heading Navigation URL"
                                                    fieldLabel="Heading Navigation URL"
                                                    name="./headingNavigationUrl"/>
                                                <subtitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Add Sub Titles here"
                                                    fieldLabel="Sub Title"
                                                    name="./subtitles">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                        name="./subtitles">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <subHeading
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Add Sub Title Title"
                                                                fieldLabel="Sub Heading"
                                                                name="./subHeading"/>
                                                            <subheadingNavigationUrl
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Add Sub Heading Navigation URL"
                                                                fieldLabel="Sub Heading Navigation URL"
                                                                name="./subheadingNavigationUrl"/>
                                                        </items>
                                                    </field>
                                                </subtitle>
                                            </items>
                                        </field>
                                    </menuItems>
                                </items>
                            </column>
                        </items>
                    </general>
                    <tab-2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Menu Items"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Add Menu Title"
                                        fieldLabel="Title"
                                        name="./mainHeading"/>
                                    <menuItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        cq-msm-lockable="./menuItems"
                                        fieldDescription="Click 'Add' to Menu items."
                                        fieldLabel="Menu Items"
                                        name="./menuItems">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            name="./menuItems">
                                            <items jcr:primaryType="nt:unstructured">
                                                <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Add Heading of the menu item"
                                                    fieldLabel="Heading"
                                                    name="./heading"/>
                                                <headingNavigationUrl
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="Select path of the Cart icon"
                                                    fieldLabel="Heading Navigation Url"
                                                    name="./headingNavigationUrl"
                                                    required="false"
                                                    value=""/>
                                                <subHeadings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Add Sub Headings"
                                                    fieldLabel="Sub Heading"
                                                    name="./subHeading">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                        name="./subHeading">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <subHeading
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldDescription="Add Failed Title"
                                                                fieldLabel="Sub Heading"
                                                                name="./subHeading"/>
                                                            <subHeadingNavigationUrl
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldDescription="Add Sub Headding Navigation Url"
                                                                fieldLabel="Sub Navigation Url"
                                                                name="./subHeadingNavigationUrl"/>
                                                            <nestedSubHeadings
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldDescription="Add Nested SubHeadings here"
                                                                fieldLabel="Nested SubHeadings"
                                                                name="./nestedSubHeadings">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                                    name="./nestedSubHeadings">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <nestedSubHeading
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldDescription="Add nested sub heading"
                                                                            fieldLabel="Nested Sub Heading"
                                                                            name="./nestedSubHeading"/>
                                                                        <nestedSubHeadingNavigationUrl
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldDescription="Add Nested Sub Headding Navigation Url"
                                                                            fieldLabel="Nested Sub Navigation Url"
                                                                            name="./nestedSubHeadingNavigationUrl"/>
                                                                        <titles
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                            composite="{Boolean}true"
                                                                            fieldDescription="Add Nested SubHeadings here"
                                                                            fieldLabel="Level 3 Titles"
                                                                            name="./titles">
                                                                            <field
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                                                name="./titles">
                                                                                <items jcr:primaryType="nt:unstructured">
                                                                                    <title
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                        fieldDescription="Add Title"
                                                                                        fieldLabel="Title"
                                                                                        name="./title"/>
                                                                                    <titleNavigationUrl
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                        fieldDescription="Add Title Navigation Url"
                                                                                        fieldLabel="Add Navigation Url"
                                                                                        name="./titleNavigationUrl"/>
                                                                                </items>
                                                                            </field>
                                                                        </titles>
                                                                    </items>
                                                                </field>
                                                            </nestedSubHeadings>
                                                        </items>
                                                    </field>
                                                </subHeadings>
                                            </items>
                                        </field>
                                    </menuItems>
                                </items>
                            </column>
                        </items>
                    </tab-2>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
